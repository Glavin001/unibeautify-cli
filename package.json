{
  "name": "@unibeautify/cli",
  "version": "0.4.0",
  "description": "CLI for Unibeautify",
  "main": "dist/index.js",
  "bin": {
    "unibeautify": "dist/cli.js"
  },
  "scripts": {
    "compile": "tsc",
    "clean": "rimraf dist",
    "jest": "jest --verbose",
    "test": "npm-run-all build jest",
    "docs": "typedoc --out docs --includeDeclarations",
    "prepare": "npm run build",
    "build": "npm-run-all clean compile",
    "lint:src": "tslint src/**/*.ts",
    "lint": "npm-run-all clean compile lint:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unibeautify/unibeautify-cli.git"
  },
  "keywords": [
    "cli-app",
    "cli",
    "unibeautify",
    "beautify",
    "beautifier",
    "formatter",
    "format",
    "pretty"
  ],
  "author": {
    "name": "Glavin Wiechert",
    "email": "glavin.wiechert@gmail.com",
    "url": "https://github.com/glavin001"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Unibeautify/unibeautify-cli/issues"
  },
  "homepage": "https://github.com/Unibeautify/unibeautify-cli#readme",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@types/cosmiconfig": "5.0.3",
    "@types/jest": "23.3.1",
    "@types/yargs": "11.1.1",
    "@unibeautify/beautifier-eslint": "0.5.0",
    "eslint": "5.3.0",
    "jest": "23.4.2",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "ts-jest": "23.1.3",
    "tslint": "5.11.0",
    "typedoc": "0.11.1",
    "typescript": "3.0.1"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "cosmiconfig": "^5.0.5",
    "g-search": "^0.3.0",
    "requireg": "^0.1.8",
    "unibeautify": "^0.16.0",
    "yargs": "^12.0.1"
  }
}
